[user]
	name = Jan Dudulski
	email = jan@dudulski.pl
[core]
	editor = nvim
	excludesfile = ~/.gitignore_global
[color]
	ui = auto
[push]
	default = current
[branch]
	autosetupmerge = true
	autosetuprebase = always
[merge]
	ff = true
[init]
	templatedir = /home/yanoo/.git_template
[rerere]
	enabled = false
[help]
	autocorrect = 1
[alias]
	co = checkout
	st = status
	cache = diff --cached
	upall = submodule foreach git pull origin master
	ignore = update-index --assume-unchanged
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
